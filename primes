#include <iostream>  
#include <stack>
using namespace std;  
int main()  
{  
  int n, i, m=0, flag=0;  
  cout << "Enter the Number to check Prime: ";  
  cin >> n;  
  m=n/2;  
  for(i = 2;; i++)  
  {  
      if(n % i != 0)  
      {  
          cout<<"Number NOT Prime."<<endl;  
          flag=1;  
          break;  
      }  
  }  
  if (flag==0)  
      cout << "Number IS Prime."<<endl;  
  return 0;  
}
